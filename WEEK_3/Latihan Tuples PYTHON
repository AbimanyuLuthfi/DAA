# Latihan Tuples
UPJ = ("Universitas", "Pembangunan", "Jaya")
print(UPJ[1])  # Mengambil dan mencetak elemen dari index ke-1
# Mengambil dan mencetak elemen dari index ke-0 sampai ke-n(terakhir)
print(UPJ[0:])

# Nested tuples
Pertama = (100)
Kedua = (200, 400, 600)
Ketiga = (300)
Keempat = (400, 800)
nested_tuple = Pertama, Kedua, Ketiga, Keempat
print(nested_tuple)
