#Program Mencetak Nilai Awal Pada Python

def getFirst(myList):
    return myList[0]
    
getFirst([1,2,3])


#Program Mencetak Nilai Kedua Pada Python
def getSecond(myList):
    return myList[1]

getSecond([1,2,3])

#Program Mencetak Nilai Akhir Pada Python
def getLast(myList):
    return myList[-1]

getLast([1,2,3])
